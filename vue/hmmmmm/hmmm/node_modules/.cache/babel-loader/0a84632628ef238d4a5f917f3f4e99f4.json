,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,UAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,UAAA,CAAA,KAAA;AACA,OAJA;AAKA,KAxBA;AAyBA;AACA,IAAA,MA1BA,oBA0BA;AACA,WAAA,UAAA,CAAA,OAAA,GAAA,CAAA;AACA,WAAA,OAAA;AACA,KA7BA;AA8BA;AACA,IAAA,KA/BA,mBA+BA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,WAAA;AACA,WAAA,MAAA;AACA,KAlCA;AAmCA;AACA,IAAA,SApCA,qBAoCA,EApCA,EAoCA;AAAA;;AACA,MAAA,aAAA,CAAA;AAAA,QAAA,EAAA,EAAA;AAAA,OAAA,CAAA,CAAA,IAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,SAAA;;AACA,QAAA,MAAA,CAAA,OAAA;AACA,OAHA;AAIA,KAzCA;AA0CA;AACA,IAAA,MA3CA,kBA2CA,EA3CA,EA2CA;AAAA;;AACA,WAAA,QAAA,CAAA,mBAAA,EAAA,OAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAKA,IALA,CAKA,YAAA;AACA,QAAA,cAAA,CAAA;AAAA,UAAA,EAAA,EAAA;AAAA,SAAA,CAAA,CAAA,IAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,SAAA;;AACA,UAAA,MAAA,CAAA,MAAA;AACA,SAHA;AAIA,OAVA,EAWA,KAXA,CAWA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,OAbA;AAcA,KA1DA;AA2DA,IAAA,GA3DA,iBA2DA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA,OAAA,CAAA,aAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA,OAAA,CAAA,IAAA,GAAA;AACA,QAAA,QAAA,EAAA,EADA;AAEA,QAAA,KAAA,EAAA,EAFA;AAGA,QAAA,KAAA,EAAA,EAHA;AAIA,QAAA,OAAA,EAAA,EAJA;AAKA,QAAA,MAAA,EAAA,EALA;AAMA,QAAA,MAAA,EAAA;AANA,OAAA;AAQA,KAtEA;AAuEA,IAAA,IAvEA,gBAuEA,GAvEA,EAuEA;AACA,WAAA,IAAA,GAAA,MAAA;AACA,WAAA,KAAA,CAAA,OAAA,CAAA,aAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA,OAAA,CAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,GAAA,CAAA,CAAA;AACA;AA3EA,GApBA;AAiGA,EAAA,OAjGA,qBAiGA;AACA,SAAA,OAAA;AACA;AAnGA,CAAA","sourcesContent":["<template>\r\n  <div class=\"user\">\r\n    <el-card style=\"margin-bottom: 20px\">\r\n      <el-form inline :model=\"form\" ref=\"form\">\r\n        <el-form-item label=\"用户名称\" prop=\"username\">\r\n          <el-input v-model=\"form.username\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"用户邮箱\" prop=\"email\">\r\n          <el-input v-model=\"form.email\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"角色\" prop=\"role_id\">\r\n          <el-select v-model=\"form.role_id\" placeholder=\"请选择角色\">\r\n            <el-option\r\n              v-for=\"(item, index,key) in $store.state.roleObj\"\r\n              :key=\"key\"\r\n              :value=\"+index\"\r\n              :label=\"item\"\r\n            ></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"search\">搜索</el-button>\r\n          <el-button @click=\"reset\">清除</el-button>\r\n          <el-button type=\"primary\" class=\"el-icon-plus\" @click=\"add\">新增用户</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-card>\r\n    <el-card>\r\n      <el-table border :data=\"tableData\">\r\n        <el-table-column label=\"序号\">\r\n          <template slot-scope=\"scope\">{{(pagination.current-1)*pagination.pageSize+scope.$index+1}}</template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"username\" label=\"用户名\"></el-table-column>\r\n        <el-table-column prop=\"phone\" label=\"电话\"></el-table-column>\r\n        <el-table-column prop=\"email\" label=\"邮箱\"></el-table-column>\r\n        <el-table-column prop=\"role\" label=\"角色\"></el-table-column>\r\n        <el-table-column prop=\"remark\" label=\"备注\"></el-table-column>\r\n        <el-table-column prop=\"status\" label=\"状态\">\r\n          <template slot-scope=\"scope\">\r\n            <div :class=\"{red:scope.row.status==0}\">{{scope.row.status==1?'启用':'禁用'}}</div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"text\" @click=\"edit(scope.row)\">编辑</el-button>\r\n            <el-button\r\n              type=\"text\"\r\n              :class=\"{red:scope.row.status==1}\"\r\n              @click=\"setStatus(scope.row.id)\"\r\n            >{{scope.row.status==1?'禁用':'启用'}}</el-button>\r\n            <el-button type=\"text\" @click=\"remove(scope.row.id)\">删除</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <div class=\"pageBox\">\r\n        <el-pagination\r\n          @size-change=\"pageChange\"\r\n          @current-change=\"currentChange\"\r\n          :page-size=\"pagination.pageSize\"\r\n          :current-page=\"pagination.current\"\r\n          :total=\"pagination.total\"\r\n          :page-sizes=\"[1,2,3,4]\"\r\n          layout=\"total,sizes,prev,pager,next,jumper\"\r\n        ></el-pagination>\r\n      </div>\r\n    </el-card>\r\n    <addUser :mode=\"mode\" ref=\"addUser\" @search='search'></addUser>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport addUser from \"./addUser\";\r\nimport { getUserData, setUserStatus, removeUserData } from \"@/api/userList\";\r\nexport default {\r\n  components: {\r\n    addUser\r\n  },\r\n  data() {\r\n    return {\r\n      mode: \"add\",\r\n      tableData: [],\r\n      pagination: {\r\n        pageSize: 2,\r\n        current: 1,\r\n        total: 4\r\n      },\r\n      form: {\r\n        username: \"\",\r\n        email: \"\",\r\n        role_id: \"\"\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    // 改变页尺寸\r\n    pageChange(size) {\r\n      this.pagination.pageSize = size;\r\n      this.search();\r\n    },\r\n    // 改变页码\r\n    currentChange(current) {\r\n      this.pagination.current = current;\r\n      this.getData();\r\n    },\r\n    // 获取数据\r\n    getData() {\r\n      let _params = {\r\n        page: this.pagination.current,\r\n        limit: this.pagination.pageSize,\r\n        ...this.form\r\n      };\r\n      // 请求数据\r\n      getUserData(_params).then(res => {\r\n        // window.console.log(res)\r\n        this.tableData = res.data.items;\r\n        this.pagination.total = res.data.pagination.total;\r\n      });\r\n    },\r\n    // 搜索\r\n    search() {\r\n      this.pagination.current = 1;\r\n      this.getData();\r\n    },\r\n    // 清除\r\n    reset() {\r\n      this.$refs.form.resetFields();\r\n      this.search();\r\n    },\r\n    // 改变状态\r\n    setStatus(id) {\r\n      setUserStatus({ id }).then(() => {\r\n        this.$message.success(\"状态设置成功!\");\r\n        this.getData();\r\n      });\r\n    },\r\n    // 删除\r\n    remove(id) {\r\n      this.$confirm(\"此项操作将删除该项数据,是否删除?\", \"温馨提示!\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"info\"\r\n      })\r\n        .then(() => {\r\n          removeUserData({ id }).then(() => {\r\n            this.$message.success(\"数据删除成功!\");\r\n            this.search();\r\n          });\r\n        })\r\n        .catch(() => {\r\n          this.$message.info(\"已取消删除\");\r\n        });\r\n    },\r\n    add() {\r\n      this.mode = \"add\";\r\n      this.$refs.addUser.dialogVisible = true\r\n      this.$refs.addUser.form = {\r\n        username: \"\",\r\n        eamil: \"\",\r\n        phone: \"\",\r\n        role_id: \"\",\r\n        status: \"\",\r\n        remark: \"\"\r\n      };\r\n    },\r\n    edit(row) {\r\n      this.mode = \"edit\";\r\n      this.$refs.addUser.dialogVisible = true\r\n      this.$refs.addUser.form = JSON.parse(JSON.stringify(row));\r\n    }\r\n  },\r\n  created() {\r\n    this.getData();\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang='less'>\r\n.user {\r\n  .pageBox {\r\n    text-align: center;\r\n    margin-top: 20px;\r\n  }\r\n  .red {\r\n    color: red;\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/view/home/userList"}]}